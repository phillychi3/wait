<div class="homepage">
    <%- partial('_partial/header') %>
    <% if(type=="index"){ %>
        <%- partial('_partial/headerimage',{height:'headfill',type:type}) %>
    <% }else{ %>
        <%- partial('_partial/headerimage',{height:'head40',type:type}) %>
    <% } %>
    <div class="issvg">
        <svg>
            <defs>
                <linearGradient id="grad1" x1="0%" y1="0%" x2="0%" y2="100%">
                    <stop offset="0%" style="stop-color:rgba(0, 0, 0, 0);stop-opacity:1" />
                    <stop offset="100%" style="stop-color:var(--backgroung-color);stop-opacity:1" />
                </linearGradient>
            </defs>
            <rect width="100%" height="100%" fill="url(#grad1)" />
        </svg>
    </div>

</div>